         <imagePainter method="arrowButtonForeground" path="images/buttons/arrow_rightO.png"
                       sourceInsets="3 3 3 3" direction="east"/>
      </state>

      <state value="MOUSE_OVER">
         <property key="ArrowButton.size" type="integer" value="16"/>
